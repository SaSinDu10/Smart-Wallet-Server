{
    "compilerOptions": {
        "target": "ESNext",
        "outDir": "js",
        "rootDir": "ts",
        "allowJs": false,
        "declaration": false,
        "declarationMap": false,
        "sourceMap": true,
        "removeComments": true,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}